@model ISAD251_WebApp.Models.Deadline

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h

<div>
    <h4>Deadline</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Title
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DeadlineTitle)
        </dd>
        <dt class = "col-sm-2">
            Date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DeadlineDate)
        </dd>
        <dt class = "col-sm-2">
            Notes
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DeadlineNotes)
        </dd>
        <dt class = "col-sm-2">
            Completed?
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IsCompleted)
        </dd>
        <dt class = "col-sm-2">
            User
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.User.UserName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.DeadlineId">Edit</a> |
